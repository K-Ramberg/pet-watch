<div id="<%= dom_id account %>">
  <p>
    <strong>Name:</strong>
    <%= account.name %>
  </p>

  <p>
    <strong>Base service fee:</strong>
    <%= number_to_currency(account.base_service_fee) %>
  </p>

  <p>
    <strong>Minimum bookable time:</strong>
    <%= account.minimum_bookable_time %>
  </p>

  <p>
    <strong>Maximum bookable time:</strong>
    <%= account.maximum_bookable_time %>
  </p>

</div>
