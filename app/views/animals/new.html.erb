<h1>New Animal</h1>

<div style="margin-bottom: 20px;">
  <% if @account %>
    <%= link_to "Back to animals", account_animals_path(@account) %> |
    <%= link_to "Back to account", account_path(@account) %>
  <% else %>
    <%= link_to "Back to animals", animals_path %>
  <% end %>
</div>

<%= render "form", animal: @animal, form_url: @account ? account_animals_path(@account) : nil %>
